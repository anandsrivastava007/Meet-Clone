    <script>
    
   

var ym = document.getElementById('your_msg');
    var chat_name ="";
    
    function _sendMsg(){
        var MyTime = new Date();
        var MyTimeStr = '';
        
        
        if(chat_name.length < 3){
            chat_name = 'Anand kumar';
        }
        
      
        MyTimeStr = ('0' + MyTime.getHours()).slice(-2) + ':'
             + ('0' + (MyTime.getMinutes())).slice(-2) + ':'
             + MyTime.getSeconds();
    
        //Message to append to char window.    
        var s0 = '<div class="message">';
        s0+= '<img src="">';
        s0+= '<div class="chat-body">';
        s0+= '<span class="chat-time">'+ MyTimeStr + '</span>';
        s0+= '<h5>' + chat_name + '</h5>';
        
        if(ym !== null){
            //Append users message.
            s0+= '<p>' + ym.value + '</p>';
        }
        
        s0+= '</div>';
        s0+= '</div>';
        
        var o = document.getElementById('msg');
        
        if(o !== null){
            //Add new message to chat window.
            o.innerHTML += s0;
            //Clear input box
            ym.value = '';
        }
}
    
    if(kp !== null){
        //Add key press event
        ym.addEventListener('keypress',kp);
    }
    
    function kp(event){
        if(event.which == 13 && ym.value.length > 0){
            //call send message function.
            _sendMsg();
        }
    }

</script>
        
    
    
